<script type="text/template" id="addEstateModel">
    <form id="form_estate">
        <table>
            <colgroup>
                <col width="300">
                <col width="120">
                <col width="380">
            </colgroup>
            <tr>
                <td colspan="2">
                    <div class="line-div w100">
                        <div class="line-div-label">所在地区：<i class="red">*</i></div>
                        <div class="line-div-center areaSelect">
                            <div id="areaBox" style="width: 100%"></div>
                        </div>
                    </div>
                </td>
                <td>
                    <div class="line-div w100">
                        <div class="line-div-label">具体位置：<i class="red">*</i></div>
                        <div class="line-div-center mapArea" style="position: relative;">
                            <input type="text" style="display:none;" name="">
                            <input type="text" placeholder="请搜索" validate
                                value="{{#nameAndTitle subdistrictName subdistrictAddress}}{{/nameAndTitle}}"     
                                class="form-control addressDetail" >
                            <input type="hidden" name="regionAddressId" value="{{regionAddressId}}">
                            <div class="areaDrop" style="width: 280px;">
                                <ul class="mapAreaNav"></ul>
                                <div class="notFound">找不到小区？点击这里添加小区</div>
                            </div>
                        </div>
                    </div>
                </td>
            </tr>
            <tr>
                <td colspan="2">
                    <div class="line-div w100">
                        <div class="line-div-label">所属板块：<i class="red">*</i></div>
                        <div class="line-div-center areaSelect">
                            <select name="zoneId" id='zoneId' class="form-control" style="width: 100%;">
                            </select>
                        </div>
                    </div>
                </td>
                <td>
                    <div class="line-div w100">
                        <div class="line-div-label">品牌公寓：<i class="red">*</i></div>
                        <div class="line-div-center">
                            <input type="text" placeholder="例如：麦家公寓靖源店" value="{{estateName}}" validate 
                                name="estateName" class="form-control " >
                        </div>
                    </div>
                </td>
            </tr>
            <tr class="addMoreInfo hide">
                <td>
                    <div class="line-div w100">
                        <div class="line-div-label">看房电话：</div>
                        <div class="line-div-center">
                            <input type="text" name="contactName"
                                class="form-control" placeholder="联系人" 
                                value="{{contactName}}">
                        </div>
                    </div>
                </td>
                <td style="padding-left: 10px;">
                    <div class="line-div">
                        <div class="line-div-center">
                            <select name='contactGender' class="form-control">
                                {{#genderTmp contactGender}}{{/genderTmp}}
                            </select>
                        </div>
                    </div>
                </td>
                <td style="padding-left: 10px;">
                    <div class="line-div">
                        <div class="line-div-center">
                            <input type="text" placeholder="联系电话" 
                                class="form-control" name="contactMobile" value="{{contactMobile}}">
                        </div>
                    </div>
                </td>
            </tr>
            <tr class="addMoreInfo hide" style="height: 70px;vertical-align: top;">
                <td colspan="3">
                    <div class="line-div w100">
                        <div class="line-div-label">公寓简介：</div>
                        <div class="line-div-center">
                            <textarea maxlength="500" 
                                placeholder="控制在500个中文字内" 
                                name="introduction" 
                                class="description form-control">{{introduction}}</textarea>
                        </div>
                    </div>
                </td>
            </tr>
            <tr class="addMoreInfo hide">
                <td colspan="3" class="text-left">
                    <div class="line-div w100">
                        <div class="line-div-label">配置/照片：</div>
                        <div class="line-div-center">
                            <button type="button" class="btn btn-mj estatePicList" style="margin-left: 0;">
                                <span class="btnText">上传照片</span>
                                <span class="badge" style="{{#showBadge cityId}}{{/showBadge}}"> {{estatePictureVOs.length}}</span>
                            </button>
                            <button type="button" class="btn btn-mj servicesElement">
                                <span class="btnText">公共基础设施</span>
                            </button>
                        </div>
                    </div>
                </td>
            </tr>
        </table>
    </form>
</script>
