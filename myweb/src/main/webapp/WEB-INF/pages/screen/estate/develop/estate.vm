<script type="text/template" id="defineFloorModel">
    <ul class="floor_defineBox maxHeightBox">
        {{#each this}}
        <li>
            <div class="input-group left" style="width: 40%;padding-right: 10px;">
                <span class="input-group-addon">楼层名称</span>
                <input type="text" validate name="floorName" class="form-control" value="{{floorName}}">
            </div>
            <div class="input-group left" style="width: 30%;padding-right: 10px;"> 
                <span class="input-group-addon">共</span>
                <input type="text" validate name="roomNum" class="form-control validateNum" 
                    value="{{roomNum}}" maxlength="3">
                <span class="input-group-addon">间</span>
            </div>            
            <div class="input-group left" style="width: 30%;text-align: left;">
                {{#showStart this}}{{/showStart}}
            </div>
            
        </li>
        {{/each}}
    </ul>
    <div class="text-left">
        <button type="button" class="btn btn-mj btn_addDefineFloor" style="margin-left: 0;">添加楼层</button>
        <button type="button" class="btn btn-active btn_shortCutFloor">使用快速添加</button>
        <button type="button" class="btn btn-mj btn_sortFloor hide">楼层排序</button>
    </div>
</script>
<script type="text/template" id="addFloor_define">
