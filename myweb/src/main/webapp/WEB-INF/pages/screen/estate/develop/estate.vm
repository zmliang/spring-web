#parse('common/top.vm')
#parse('common/header.vm')
#parse('common/left.vm')
<div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12 window">
        <div class="x_panel window">
            <div class="x_content clearfix">
                <div class="content_box left" style="width: 100%;">
                    <div class="left" style="width: 240px;margin-right: 10px;">
                        <div class="clearfix" style="margin-bottom: 10px;">
                            <select name="cityId" class="width-auto left">
                                <option value=""></option>
                            </select>
                            <button type="button" class="btn btn-mj right" id="addEstate">新建公寓</button>
                        </div>
                        <div class="list-group list-group-nowrap" id="estateBox" style="padding:5px 0;border-radius:4px;margin-bottom: 0;">
                            <!-- <a href="javascript:;" class="list-group-item active">
                                <span class="badge">141</span>
                                柏林公寓
                            </a> -->
                        </div>
                    </div>
                    <div class="content_right-box" style="overflow: hidden;">
                        <div class="panel panel-default estateInfoBox hide" style="margin-bottom: 10px;">
                            <div class="panel-heading" style="padding:0 10px;height:40px;background: #eef0f6">
                                <h2 id="estateFullName" style="display: inline-block;"><!-- 麦家公寓新华店 --></h2>
                                <span id="estateAddrDetail"><!-- 杭州市-西湖区-西溪路138号靖源国际 --></span>
                                <div class="clearfix right" style="padding: 5px 0;">
                                    <button type="button" class="btn btn-mj" id="editEstate">编辑公寓</button>
                                    <button type="button" class="btn btn-danger" id="delEstate">删除公寓</button>
                                </div>
                            </div>
                            <div class="panel-body" style="padding: 10px;">
                                <div class="btn-group">
                                    <button type="button" class="btn btn-active" id="downPayment">预定金</button>
                                    <button type="button" class="btn btn-active" id="energyCosts">能耗费用</button>
                                    <button type="button" class="btn btn-active" id="configure">租赁配置</button>
                                    <button type="button" class="btn btn-active hide" id="applyFinance">金融申请</button>
                                    <button type="button" class="btn btn-active" id="publicApp">公寓推广</button>
                                </div>
                            </div>
                        </div>
                        <div class="panel panel-default estateInfoBox_noData text-center" style="margin-bottom: 10px; line-height: 70px;">
                            <div class="panel-body" style="padding: 10px;">
                                数据加载中，请稍后...
                            </div>
                        </div>
                        <div class="panel panel-default" style="margin-bottom: 0;">
                            <div class="panel-body" style="padding: 10px;">
                                <form id="searchForm" class="clearfix" style="margin: -5px 0 5px 0;">
                                    <div class="searDiv left">
                                        <table>
                                            <colgroup>
                                                <col width="120">
                                                <col width="120">
                                                <col width="150">
                                                <col width="180">
                                            </colgroup>
                                            <tbody>
                                                <tr><td>
                                                    <div class="line-div">
                                                        <div class="line-div-label"></div>
                                                        <div class="line-div-center">
                                                            <select name="floorId" class="default form-control">
                                                                <option value=""></option>
                                                            </select>
                                                        </div>
                                                    </div></td>
                                                    <td style="padding-left: 10px;">
                                                    <div class="line-div">
                                                        <div class="line-div-label"></div>
                                                        <div class="line-div-center">
                                                            <select name="rentNo" class="default form-control">
                                                                <option value=""></option>
                                                                <option value="1">已出租</option>
                                                                <option value="2">未出租</option>
                                                            </select>
                                                        </div>
                                                    </div></td>
                                                    <td style="padding-left: 10px;"><div class="line-div">
                                                        <div class="line-div-label"></div>
                                                        <div class="line-div-center">
                                                            <input type="text" name="roomNo" class="form-control" value="" placeholder="房号" >
                                                        </div>
                                                    </div></td>
                                                    <td class="text-left">
                                                        <button type="button" id="search" class="left btn btn-mj">查询</button>
                                                        <button type="button" id="clear" class="left btn">清空</button>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                    <div class="right" style="padding: 5px 0">
                                        <button type="button" class="btn btn-mj" id="appRelease" style="margin:0 -8px">发布到APP</button>
                                        <button type="button" class="btn btn-mj" id="appBackout">从APP撤销</button>
                                        <button type="button" class="btn btn-mj" id="addNewRoom">新建房号</button>
                                        <button type="button" class="btn btn-danger" id="delRoom"style="margin:0 -3px">删除房号</button>
                                    </div>
                                </form>
                                <div id="gridDiv"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
