</script>
<!-- 应用到房间 -->
<script type="text/template" id="applyToRoomsModel">
    <div class="col-sm-12">
        <label for="" class="col-sm-4"><input type="checkbox" disabled="" checked> 当前房间已是该房型</label>
        <label for="" class="col-sm-4"><input type="checkbox" onclick="return false">当前房间尚未选择该房型</label>
        <label for="" class="col-sm-4"><input type="checkbox" checked onclick="return false"> 当前房间正在选择该房型</label>
    </div>
    <div class="inner-box" id='box' style="float: left">
        <div class="title">
            请选择要应用该房型的房间
            <button type="button" class="btn btn-right" id='btn-change2' chooseflag>全选/全不选</button>
        </div>
        <div class="roomNo-box" style='padding:20px 0;'>
            {{#each this}}
            <div class="roomNo-item" >
                <div class="room-floor">
                    <label class="checkboxItem">
                        <input type="checkbox" name="" style="position: relative; top:-1px;">
                        {{floor}}
                    </label>
                </div>
                <div class="room-no-item">
                    {{#each roomItem}}
                    <div class="room-item" style='margin: 9px 0;'>
                        <label class="checkboxItem">
                            <input type="checkbox" name="" value='{{roomId}}' 
                                style="position: relative; top:-1px;" 
                                {{#checkedRoomId roomId checkedRooms}}{{/checkedRoomId}}
                                {{#disabledRoomId roomId checkedRooms}}{{/disabledRoomId}}
                            > 
                            {{roomNo}}
                        </label>
                    </div>
                    {{/each}}
                </div>
            </div>
            {{/each}}
        </div>
    </div>
</script>
<script type="text/template" id="friendlyTipsModel">
    <button type="button" class="btn btn-active addMoreBtn" style="margin-top: 10px;">填写更多信息</button>
</script>

<!--房号 -->
<script type="text/template" id="addAndEditRoom">
    <form id='J_form'>
        <div class="col-sm-6" style="display:{{display}}">       
            <div class="line-div w75">
                <div class="line-div-label">
                    公寓:<i class="red">*</i>
                </div>
                <div class="line-div-center">
                    <input type="text" class="form-control" name="" value="{{estateName}}" readonly>
                </div>
            </div>
        </div>
        <div class="col-sm-6">       
            <div class="line-div w75">
                <div class="line-div-label">
                    楼层:<i class="red">*</i>
                </div>
                <div class="line-div-center">
                    <select name="floorId" id='floor'>
                    </select>
                </div>
            </div>
        </div>
        <div class="col-sm-6">      
            <div class="line-div w75" id='roomNo'>
                <div class="line-div-label">
                    起始房号:<i class="red">*</i>
                </div>
                <div class="line-div-center">
                    <input type="text" validate name='startNo' placeholder="例：101" maxlength="5"
                        value='{{roomNo}}' class="form-control validateNum">
                </div>
            </div>
        </div>
        <div class="col-sm-6" style="display:{{display}}">   
            <div class="line-div w75">
                <div class="line-div-label">
                    末尾房号:
                </div>
                <div class="line-div-center">
                    <input type="text" name='endNo' placeholder="例：120"  maxlength="5"
                        value='{{endNo}}' class="form-control validateNum">
                </div>
            </div>
        </div>
        <div class="col-sm-6" style="display:{{display}}">   
            <div class="line-div w75">
                <div class="line-div-label">
                    跳过尾号:
                </div>
                <div class="line-div-center">
                    <select name="defaultNoStr" class="form-control" multiple="multiple">
                        <option value="0">0</option>
                        <option value="1">1</option>
                        <option value="2">2</option>
                        <option value="3">3</option>
                        <option value="4">4</option>
                        <option value="5">5</option>
                        <option value="6">6</option>
                        <option value="7">7</option>
                        <option value="8">8</option>
                        <option value="9">9</option>
                    </select>
                </div>
            </div>
        </div>
        <div class="col-sm-6">   
            <div class="line-div w75">
                <div class="line-div-label">
                    房间类型:<i class="red">*</i>
                </div>
                <div class="line-div-center">
                    <select name="roomTypeId" id='typeId'></select>
                </div>
            </div>
        </div>
        <div class="col-sm-6" style="display:{{undisplay}}">  
            <div class="line-div w75">
                <div class="line-div-label">
                    房间照片:
