<!-- 房类 -->
<script type="text/template" id="addRoomModel">
    <form id="form_room">
        <div class="line-div w100">
            <div class="line-div-label">房间类型：<i class="red">*</i></div>
            <div class="line-div-center roomTypeBox clearfix">
                <ul class="list-group maxHeightBox" style="margin-bottom: 0;">
                    {{#each this}}
                    <li class="col-md-12 list-group-item" style="border: 0 none;padding: 0 10px 10px 0;">
                        <div class="input-group" style="margin-bottom: 0;">
                            <input type="text" placeholder="房间类型名称" value="{{styleName}}" validate 
                                name="styleName" class="form-control" >
                            <span class="input-group-btn">
                                <button type="button" class="btn btn-active roomTypePicList">
                                    <span class="btnText">上传照片</span>
                                    <span class="badge" style="{{#showBadge roomTypeId}}{{/showBadge}}"> {{roomTypePicture.length}}</span>
                                </button>
                                <button type="button" class="btn btn-active applyToRooms" style="width: 150px;">
                                    <span class="btnText">应用到房间</span>
                                    <span class="badge" style="{{#showBadge roomTypeId}}{{/showBadge}}"> {{targetRoomIds.length}}</span>
                                </button>
                                <button type="button" class="btn btn-active deleteRoomType" style="min-width: initial">
                                    <span class="icon-delete"></span>
                                </button>
                            </span>
                        </div>
                    </li>
                    {{/each}}
                </ul>
                <div class="text-left">
                    <button type="button" class="btn btn-mj btn_addRoomType" style="margin-left: 0;">添加房间类型</button>
                </div>
            </div>
        </div>
    </form>
</script>
<script type="text/template" id="addRoomTypeModel">
    <li class="col-md-12 list-group-item" style="border: 0 none;padding: 0 10px 10px 0;">
        <div class="input-group" style="margin-bottom: 0;">
            <input type="text" placeholder="房间类型名称" value="" validate 
                name="styleName" class="form-control" >
            <span class="input-group-btn">
                <button type="button" class="btn btn-active roomTypePicList">
                    <span class="btnText">上传照片</span>
                    <span class="badge"> 0</span>
                </button>
                <button type="button" class="btn btn-active applyToRooms" style="width: 150px;">
                    <span class="btnText">应用到房间</span>
                    <span class="badge"> 0</span>
                </button>
                <button type="button" class="btn btn-active deleteRoomType" style="min-width: initial">
                    <span class="icon-delete"></span>
                </button>
            </span>
        </div>
