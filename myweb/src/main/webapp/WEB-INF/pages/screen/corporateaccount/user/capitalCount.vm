#parse('common/top.vm')
#parse('common/header.vm')
#parse('common/left.vm')
<div class="x_panel window anti-window">
    <div class="col-md-12 col-sm-12 col-xs-12 window" style="padding: 0">
        <div class="x_panel">
            <div class="label_btm_line clearfix">
                <input type="hidden" id="financeTrusteeshipType" value="$!{financeTrusteeshipType}">
                <input type="hidden" id="self_organizationId" value="$!{organizationId}">
                <strong class="left">我的账户</strong>
                <div class="clearfix">
                    <div class="right finger">银行卡：&nbsp;
                        <em class="warning hide" id="btn-watchcard">查看</em>
                        <em class="hide" id="btn-bindcard">绑定</em>
                    </div>
                    <div id="btn-explaination" class="btn-withdraw right" style="position:initial;margin-right: 10px;">提现说明</div>
                    #if ($!{financeTrusteeshipType})
                    <div id="btn-transfer" class="btn-withdraw right" style="position:initial;margin-right: 10px;">我要转账</div>
                    #end
                </div>
            </div>
            <div class="m_4wrapper">
                <div class="item">
                    <div class="tit">
                        <div class="left">账户余额</div>
                        <em class="ques" id="ques_balance" tips="这里的余额是租客通过平台线上支付渠道，支付了租赁产生的费用"></em>
                    </div>
                    <div class="val">
                        <em id="label-balance">0</em>元
                    </div>
                </div>
                <div class="item">
                    <div class="tit">
                        <div class="left">可提现金额</div>
                        <div id="btn-withdraw" class="btn-withdraw">立即提现</div>
                    </div>
                    <div class="val">
                        <em id="label-canWithdraw">0</em>元
                    </div>
                </div>
                <div class="item">
                    <div class="tit">
                        <div class="left">提现中金额</div>
                    </div>
                    <div class="val">
                        <em id="label-applyMoney">0</em>元
                    </div>
                </div>
                <div class="item">
                    <div class="tit">
                        <div class="left">资金余额</div>
                        <em class="ques" id="ques_frozenAmount" tips="资金余额：表示 2017-12-15 00：00：00  之前的账单已结算完毕，当前已无法通过按账单笔数提现；资金余额里面的资金目前只支持一次性提现。"></em>
                        <div id="btn-history" class="btn-withdraw">全部提现</div>
                    </div>
                    <div class="val">
                        <em id="label-frozenAmount">0</em>元
                    </div>
                </div>
            </div>
        </div>
        <div id="panel2" class="x_panel" style="margin-top: 5px;">
            <ul class="nav nav-tabs resourceType">
                <li class="active"><a href="javascript:;">资金入账流水</a></li>
                #if ($!{financeTrusteeshipType})
                <li><a href="javascript:;">转账流水</a></li>
                #end
            </ul>
            <button type="button" class="btn btn-mj btn_checkedItems" 
                style="position: absolute;top: 10px; right: 10px; z-index: 99;">
                已选择账单
                <span class="badge">0</span>
            </button>
            <div class="tab-content container active" 
                style="margin-bottom: -1px;border-bottom-left-radius: 0;border-bottom-right-radius: 0;">
                <form id="searchForm" style="margin-bottom: 10px;">
                    <table style="margin: 5px 0 5px 0;">
                        <colgroup>
                            <col width="465">
                            <col width="10">
                            <col width="130">
                            <col width="10">
                            <col width="130">
                            <col width="10">
                            <col width="240">
                        </colgroup>
                        <tbody>
                            <tr>
                                <td>
                                    <div class="line-div w90">
                                        <div class="line-div-label">结账时间：</div>
                                        <div class="line-div-center">
                                            <input type="text" name="startPayTime" class="form-control left Wdate" style="width:45%" id="startPayTime" onfocus="WdatePicker({dateFmt:'yyyy-MM-dd HH:mm:ss',isShowClear:true,maxDate:'#F{$dp.$D(\'endPayTime\')}'})">
                                            <span class="left text-center" style="width:10%;"> -- </span>
                                            <input type="text" id="endPayTime" name="endPayTime" class="form-control left Wdate" style="width:45%" onfocus="WdatePicker({dateFmt:'yyyy-MM-dd HH:mm:ss',isShowClear:true,maxDate:'%y-%M-%d %H:%m:%s',minDate:'#F{$dp.$D(\'startPayTime\')}'})">
                                        </div>
                                    </div>
                                </td>
                                <td></td>
                                <td>
                                    <div class="line-div">
                                        <div class="line-div-center">
                                            <select name="rateType" class="form-control" data-nameStr="费用类型" style="width: 100%;">
                                                <option value=""></option>
                                                <option value="1">房租</option>
                                                <option value="2">押金</option>
                                                <option value="5">增配</option>
                                                <!-- <option value="6">退款</option> -->
                                                <option value="8">水费</option>
                                                <option value="9">电费</option>
                                                <option value="11">定金</option>
                                                <!-- <option value="12">退房</option> -->
                                                <option value="13">服务费</option>
                                                <option value="14">期租金</option>
                                                <option value="15">首期租金</option>
                                                <option value="7">其他</option>
                                            </select>
                                        </div>
                                    </div>
                                </td>
                                <td></td>
                                <td>
                                    <div class="line-div">
                                        <div class="line-div-center">
                                            <select  name="payChannel" class="form-control" data-nameStr="支付方式" style="width: 100%;">
                                                <option value=""></option>
                                                <option value="1">平台支付宝</option>
                                                <option value="2">平台微信</option>
                                                <option value="3">平台认证支付</option>
                                            </select>
                                        </div>
                                    </div>
                                </td>
                                <td></td>
                                <td class="text-left">
                                    <button type="button" id="search" class="btn btn-mj search" style="margin-left: 0;">查询</button>
                                    <button type="button" id="empty" class="btn empty">清空</button>
                                    <button type="button" id="export" class="btn">导出</button>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                    <table>
                        <tbody>
                            <tr>
                                <td width="130" style="padding-left: 10px;">
                                    <div class="line-div">
                                        <div class="line-div-center">
                                            <select name="cityId" class="form-control" style="width: 100%;">
                                                <option value=""></option>
                                            </select>
                                        </div>
                                    </div>
                                </td>
                                <td width="130" style="padding-left: 10px;">
                                    <div class="line-div">
                                        <div class="line-div-center">
                                            <select name="areaId" class="form-control" style="width: 100%;">
                                                <option value=""></option>
                                            </select>
                                        </div>
                                    </div>
                                </td>
                                <td width="205" style="padding-left: 10px;">
                                    <div class="line-div">
                                        <div class="line-div-center">
                                            <select name="subdistrictId" class="form-control" style="width: 100%;">
                                                <option value=""></option>
                                            </select>
                                        </div>
                                    </div>
                                </td>
                                <td width="140" style="padding-left: 10px;">
                                    <div class="line-div">
                                        <div class="line-div-center">
                                            <select name="status" class="form-control" style="width: 100%;">
                                                <option value=""></option>
                                                <option value="0">待操作</option>
                                                <option value="1">提现中</option>
                                                <option value="2">已提现</option>
                                                <option value="3">资金转出</option>
                                                <option value="4">已入资金帐</option>
                                            </select>
                                        </div>
                                    </div>
                                </td>
                                <td width="140" style="padding-left: 10px;">
                                    <div class="line-div">
                                        <div class="line-div-center">
                                            <select name="houseFinanceType" class="form-control" style="width: 100%;">
                                                <option value=""></option>
                                                <option value="1">普通房源账单</option>
                                                <option value="2">金融房源账单</option>
                                            </select>
                                        </div>
                                    </div>
                                </td>
                                <td class="transInOrgId hide" width="160" style="padding-left: 10px;">
                                    <div class="line-div">
                                        <div class="line-div-center">
                                            <select name="transInOrgId" class="form-control" data-nameStr="放款组织" style="width: 100%;">
                                                <option value=""></option>
                                            </select>
                                        </div>
                                    </div>
                                </td>
                                <td width="160" style="padding-left: 10px;">
                                    <div class="line-div">
                                        <div class="line-div-center">
                                            <input placeholder="姓名/身份证/账单号" class="form-control" name="keyWord">
                                        </div>
                                    </div>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </form>
            </div>
            <div class="table-topDiv" style="margin-top:0;">
                当前统计金额：<span class="red"></span>
            </div>
            <div class="gridBox">
                <div id="gridDiv0"></div>
            </div>
            <div class="gridBox hide">
                <div id="gridDiv1"></div>
            </div>
        </div>
    </div>
</div>

